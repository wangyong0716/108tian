<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:drawable="@drawable/bg_cornered_disabled" />

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#F7E9D4" />
            <corners android:radius="0dp" />
            <stroke
                    android:width="0.5dp"
                    android:color="#FFC568" />
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="#F7E9D4" />
            <corners android:radius="0dp" />
            <stroke
                    android:width="0.5dp"
                    android:color="#FFC568" />
        </shape>
    </item>
</selector>
